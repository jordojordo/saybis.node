{
  "name": "daphine",
  "version": "0.3.1",
  "description": "The API for Freedom",
  "author": "Jordon Leach",
  "license": "GPL-3.0-only",
  "main": "server.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/jordojordo/daphine.git"
  },
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "build": "tsc && copyfiles -u 1 src/views/**/* dist",
    "start": "node ./dist/server.js",
    "start:dev": "tsc-watch --onSuccess \"pnpm run serve\"",
    "serve": "pnpm run copy-views && pnpm run start",
    "copy-views": "copyfiles -u 1 src/views/**/* dist",
    "lint": "npx eslint .",
    "format": "npx eslint . --fix"
  },
  "dependencies": {
    "module-alias": "^2.2.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/http-errors": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.1",
    "cookie-parser": "^1.4.7",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "ejs": "^3.1.10",
    "eslint": "^9.16.0",
    "express": "^4.21.1",
    "globals": "^15.13.0",
    "http-errors": "^2.0.0",
    "morgan": "^1.10.0",
    "puppeteer": "^23.9.0",
    "tsc-watch": "^6.2.1",
    "typescript": "^5.7.2"
  },
  "_moduleAliases": {
    "@src": "dist",
    "@routes": "dist/routes",
    "@api": "dist/api"
  }
}
